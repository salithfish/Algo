public static void bubbleSort(int[] array,int n){
        for(int i = 0;i < n ;i++){
            for (int j = 0;j<n-i-1;j++){
                if(array[j]>array[j+1])
                {
                    int a = array[j];
                    array[j] = array[j+1];
                    array[j+1] = a;
                }
            }
        }
    }
